            }
          }
        ]
      },
      "mg_defend": {
        "queue_command": {
          "command": ["tag @s remove mg_defend"]
        },
        "remove": {
          "component_groups": ["normal"]
        },
        "add": {
          "component_groups": ["defend"]
        }
