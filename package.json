{
  "dependencies": {
    "@minecraft/server": "^1.9.0-beta.1.20.60-stable",
    "@minecraft/server-ui": "^1.2.0-beta",
    "npm": "^10.2.5"
  }
}



// to change
import { world, system } from "@minecraft/server";
import Gaia from './world/Gaia.js';

function showSkybox() {
    system.runTimeout(() => {
        let players = Gaia.getPlayers()
        for (const player of players) {
            const { x, y, z } = player.location;
            if (x > 1000 && z > 1000 && player.dimension.id == 'minecraft:the_end') {
                player.playAnimation('animation.skybox.gaia');
            }
        }
    }, 100);
}
showSkybox();
