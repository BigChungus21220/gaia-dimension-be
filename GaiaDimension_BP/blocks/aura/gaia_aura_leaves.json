{
  "format_version": "1.19.20",
  "minecraft:block": {
    "description": {
      "identifier": "gaia:aura_leaves",
      "properties": {
        "gaia:decay_tier": [6, 5, 4, 3, 2, 1, 0],
        "gaia:not_decay": [false, true],
        "gaia:solid": [false, true]
      },
      "menu_category": {
        "category": "nature",
        "group": "itemGroup.name.leaves"
      }
    },
    "components": {
      "minecraft:loot": "loot_tables/blocks/gaia/aura_leaves.json",
      "minecraft:on_player_placing": {
        "event": "gaia:stop_decay",
        "target": "self"
      },
      "minecraft:on_player_destroyed": {
        "event": "gaia:on_destroyed"
      },
      "minecraft:random_ticking": {
        "on_tick": {
          "event": "check",
          "target": "block"
        }
      },
      "minecraft:queued_ticking": {
        "looping": true,
        "interval_range": [0, 0],
        "on_tick": {
          "event": "check"
        }
      },
      "minecraft:destructible_by_mining": {
        "seconds_to_destroy": 0.3
      },
      "minecraft:map_color": "#FFFFFF",
      "minecraft:destructible_by_explosion": {
        "explosion_resistance": 1
      },
      "minecraft:unit_cube": {},
      "minecraft:light_dampening": 8,
      "tag:gaia:aura_leaves": {},
      "minecraft:material_instances": {
        "*": {
          "texture": "aura_leaves",
          "render_method": "blend",
          "ambient_occlusion": true,
          "face_dimming": true
        }
      }
    },
    "events": {
      "gaia:on_destroyed": {
        "sequence": [
          {
            "condition": "query.get_equipped_item_name == 'shears'",
            "spawn_loot": {
              "table": "loot_tables/blocks/gaia/aura_leaves_sheared.json"
            }
          },
          {
            "condition": "query.get_equipped_item_name != 'shears'",
            "spawn_loot": {
              "table": "loot_tables/blocks/gaia/aura_leaves.json"
            }
          }
        ]
      },
      "check": {
        "sequence": [
          {
            "condition": "q.block_property('gaia:not_decay') == false",
            "set_block_property": {
              "gaia:decay_tier": "(    q.block_neighbor_has_any_tag(0,0,-1,'gaia:aura_log') ||    q.block_neighbor_has_any_tag(0,0,1,'gaia:aura_log') ||    q.block_neighbor_has_any_tag(-1,0,0,'gaia:aura_log') ||    q.block_neighbor_has_any_tag(1,0,0,'gaia:aura_log') ||    q.block_neighbor_has_any_tag(0,-1,0,'gaia:aura_log') ||    q.block_neighbor_has_any_tag(0,1,0,'gaia:aura_log')) ?     6: (    (        q.block_neighbor_has_any_tag(0,0,-1,'gaia:decay_tier_6') ||         q.block_neighbor_has_any_tag(0,0,1,'gaia:decay_tier_6') ||         q.block_neighbor_has_any_tag(-1,0,0,'gaia:decay_tier_6') ||         q.block_neighbor_has_any_tag(1,0,0,'gaia:decay_tier_6') ||         q.block_neighbor_has_any_tag(0,-1,0,'gaia:decay_tier_6') ||         q.block_neighbor_has_any_tag(0,1,0,'gaia:decay_tier_6')    ) ?        5     : (         (            q.block_neighbor_has_any_tag(0,0,-1,'gaia:decay_tier_5') ||             q.block_neighbor_has_any_tag(0,0,1,'gaia:decay_tier_5') ||             q.block_neighbor_has_any_tag(-1,0,0,'gaia:decay_tier_5') ||             q.block_neighbor_has_any_tag(1,0,0,'gaia:decay_tier_5') ||             q.block_neighbor_has_any_tag(0,-1,0,'gaia:decay_tier_5') ||             q.block_neighbor_has_any_tag(0,1,0,'gaia:decay_tier_5')        ) ?            4         : (             (                q.block_neighbor_has_any_tag(0,0,-1,'gaia:decay_tier_4') ||                 q.block_neighbor_has_any_tag(0,0,1,'gaia:decay_tier_4') ||                 q.block_neighbor_has_any_tag(-1,0,0,'gaia:decay_tier_4') ||                 q.block_neighbor_has_any_tag(1,0,0,'gaia:decay_tier_4') ||                 q.block_neighbor_has_any_tag(0,-1,0,'gaia:decay_tier_4') ||                 q.block_neighbor_has_any_tag(0,1,0,'gaia:decay_tier_4')            ) ?                3            : (                (                    q.block_neighbor_has_any_tag(0,0,-1,'gaia:decay_tier_3') ||                     q.block_neighbor_has_any_tag(0,0,1,'gaia:decay_tier_3') ||                     q.block_neighbor_has_any_tag(-1,0,0,'gaia:decay_tier_3') ||                     q.block_neighbor_has_any_tag(1,0,0,'gaia:decay_tier_3') ||                     q.block_neighbor_has_any_tag(0,-1,0,'gaia:decay_tier_3') ||                     q.block_neighbor_has_any_tag(0,1,0,'gaia:decay_tier_3')                ) ?                    2                : (                    (                        q.block_neighbor_has_any_tag(0,0,-1,'gaia:decay_tier_2') ||                         q.block_neighbor_has_any_tag(0,0,1,'gaia:decay_tier_2') ||                         q.block_neighbor_has_any_tag(-1,0,0,'gaia:decay_tier_2') ||                         q.block_neighbor_has_any_tag(1,0,0,'gaia:decay_tier_2') ||                         q.block_neighbor_has_any_tag(0,-1,0,'gaia:decay_tier_2') ||                         q.block_neighbor_has_any_tag(0,1,0,'gaia:decay_tier_2')                    ) ?                        1                    : (                        0                    )                )            )        )     ) )"
            }
          },
          {
            "set_block_property": {
              "gaia:solid": "q.block_neighbor_has_any_tag(0,0,-1,'gaia:aura_log','gaia:aura_leaves') && q.block_neighbor_has_any_tag(0,0,1,'gaia:aura_log','gaia:aura_leaves') && q.block_neighbor_has_any_tag(0,1,0,'gaia:aura_log','gaia:aura_leaves') && q.block_neighbor_has_any_tag(0,-1,0,'gaia:aura_log','gaia:aura_leaves') && q.block_neighbor_has_any_tag(-1,0,0,'gaia:aura_log','gaia:aura_leaves') && q.block_neighbor_has_any_tag(1,0,0,'gaia:aura_log','gaia:aura_leaves')"
            }
          }
        ]
      },
      "gaia:stop_decay": {
        "set_block_property": {
          "gaia:not_decay": true
        }
      },
      "gaia:decay": {
        "die": {}
      }
    },
    "permutations": [
      {
        "condition": "query.block_property('gaia:decay_tier') == 0",
        "components": {
          "tag:gaia:decay_tier_0": {},
          "minecraft:random_ticking": {
            "on_tick": {
              "event": "gaia:decay"
            }
          }
        }
      },
      {
        "condition": "query.block_property('gaia:decay_tier') == 1",
        "components": {
          "tag:gaia:decay_tier_1": {}
        }
      },
      {
        "condition": "query.block_property('gaia:decay_tier') == 2",
        "components": {
          "tag:gaia:decay_tier_2": {}
        }
      },
      {
        "condition": "query.block_property('gaia:decay_tier') == 3",
        "components": {
          "tag:gaia:decay_tier_3": {}
        }
      },
      {
        "condition": "query.block_property('gaia:decay_tier') == 4",
        "components": {
          "tag:gaia:decay_tier_4": {}
        }
      },
      {
        "condition": "query.block_property('gaia:decay_tier') == 5",
        "components": {
          "tag:gaia:decay_tier_5": {}
        }
      },
      {
        "condition": "query.block_property('gaia:decay_tier') == 6",
        "components": {
          "tag:gaia:decay_tier_6": {}
        }
      },
      {
        "condition": "query.block_property('gaia:solid') == true",
        "components": {
          "minecraft:material_instances": {
            "*": {
              "texture": "aura_leaves_opaque",
              "render_method": "opaque"
            }
          }
        }
      }
    ]
  }
}
